{
  "firebase": "crowdlearn",
  "public": "./app",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "**/bower_components/**"
  ],
  "headers": [ 
    {
      "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
    }, {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=691200"
      } ]
    }, {
      "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=691200"
      } ]
    }, {
      "source" : "**/*.min.@(css|js)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=691200"
      } ]
    }
  ]
}